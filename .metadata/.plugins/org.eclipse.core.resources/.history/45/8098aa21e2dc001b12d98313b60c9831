package com.hexaware.airlinesreservations.Airlinesreservations.models;

import java.sql.Timestamp;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "Flights")
public class Flights {

	@Id
	@GeneratedValue
	@Column(name = "flight_id")
	private int flightID;
	
	@Column(name = "flight_name")
	private String flightName;
	
	@Column(name = "source_city")
	private String sourceCity;
	
	@Column(name = "destination_city")
	private String destinationCity;
	
	@Column(name = "seats_available")
	private int seats;
	
	@Column(name = "price")
	private int price;
	
	@Column(name = "create_user")
	private String userID;
	
	@Column(name = "create_timestmap")
	private Timestamp createTimestamp;
	
	public Flights() {
		
	}

	public Flights(int flightID, String flightName, String sourceCity, String destinationCity, int seats, int price,
			String userID, Timestamp createTimestamp) {
		super();
		this.flightID = flightID;
		this.flightName = flightName;
		this.sourceCity = sourceCity;
		this.destinationCity = destinationCity;
		this.seats = seats;
		this.price = price;
		this.userID = userID;
		this.createTimestamp = createTimestamp;
	}
	
}
